package data_board;

import java.sql.Connection;
import java.sql.SQLException;

public class MemberDAO {
	private static MemberDAO instance = null;
	private MemberDAO() {}
	
	public static MemberDAO getInstance() {
		if(instance==null) {
			synchronized (MemberDAO.class) {
				instance = new MemberDAO();
			}
		}
		return instance;
	}
